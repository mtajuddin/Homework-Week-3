void setup() {
  size(400, 400);
  background(255);
  noLoop();
}

void draw() {
  
 int y = 0;
 while(y < 400) {
   int x = 0;
   while (x < 400) {
     stroke(0);
     rectMode(CENTER);
     fill(255);
     float rand = random(0,50);
     
     if (rand < 40) {
       rect(10 + x, 10 + y, 14, 14);
     }
     x += 20;
   }
   y += 20;
 }
}
