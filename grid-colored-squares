void setup() {
  size(400, 400);
  background(255);
  noLoop();
}

void draw() {
  int y = 0;
  
  while(y < 400) {
    int x = 0;
    while(x < 400) {
      stroke(255);
      strokeWeight(4);
      rectMode(CENTER);
      fill(random(255), random(255), random(255));
      rect(10 + x, 10 + y, 20, 20);
      x += 20;
    }
    y += 20;
  }
}
