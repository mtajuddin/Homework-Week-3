int x = 0;
int y = 0;
int x1 = 0;
int y1 = 0;

void setup() {
  size(400, 400);
  background(255);
}

void draw() { 
  strokeWeight(1);
  int counter = 0;
  while (counter < 80) {
    counter = 0;
    x = 0;
    x1 = 0;

    while (counter < 80) {
      line(5 + x, 4 + y, 5 + x, 16 + y);
      line(0 + x1, 10 + y1, 10 + x1, 10 + y1);
      //line(40, 4, 40, 20);
      //line(30, 12, 50, 12);
      x += 20;
      x1 += 20;
      counter += 1;
    }
    y += 20;
    y1 += 20;
  }
}
